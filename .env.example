# Cloudflare Turnstile
NEXT_PUBLIC_TURNSTILE_SITE_KEY=<turnstile_site_key>
TURNSTILE_SECRET_KEY=<turnstile_secret_key>

# Sentry
SENTRY_DSN=<sentry_dsn_optional>

# N8N Support Integration
N8N_WEBHOOK_URL=<n8n_webhook_url_for_support_form>
N8N_WEBHOOK_MASTER_CALL=<n8n_webhook_url_for_master_contact_form>
N8N_SECRET_TOKEN=<n8n_shared_secret_value>
N8N_SECRET_HEADER=x-secret-token
