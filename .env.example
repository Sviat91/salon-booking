# Google
GOOGLE_APPLICATION_CREDENTIALS_JSON=<json_or_base64_google_service_account>
GOOGLE_DRIVE_FOLDER_ID=<google_drive_folder_id_reviews>

# Master Olga (default)
GOOGLE_CALENDAR_ID=<google_calendar_id_olga>
GOOGLE_SHEET_ID=<google_sheet_id_olga>

# Master Yuliia
GOOGLE_CALENDAR_ID_YULIIA=<google_calendar_id_yuliia>
GOOGLE_SHEET_ID_YULIIA=<google_sheet_id_yuliia>

# User Consents (shared between masters)
USER_CONSENTS_GOOGLE_SHEET_ID=<google_sheet_id_for_consents>

# Redis (Upstash)
UPSTASH_REDIS_REST_URL=<upstash_rest_url>
